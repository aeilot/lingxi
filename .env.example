# OpenAI API Configuration
OPENAI_API_KEY=your_api_key_here
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_MODEL=gpt-3.5-turbo

# Scheduler Configuration
# Check session inactivity every N minutes (default: 5)
SCHEDULER_CHECK_INTERVAL_MINUTES=5

# Celery Configuration
# Redis broker URL for Celery (default: redis://localhost:6379/0)
CELERY_BROKER_URL=redis://localhost:6379/0
# Redis result backend for Celery (default: redis://localhost:6379/0)
CELERY_RESULT_BACKEND=redis://localhost:6379/0

